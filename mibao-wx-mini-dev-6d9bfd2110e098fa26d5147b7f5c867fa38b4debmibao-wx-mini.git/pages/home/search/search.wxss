.product-wp {
  margin: 0 30rpx;
  padding: 30rpx 0;
  border-bottom: 0.5px solid #eaeaea;
}
.product-wp .product {
  position: relative;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.product-wp .product .img {
  width: 200rpx;
  height: 200rpx;
}
.product-wp .product .img image {
  width: 100%;
  height: 100%;
}
.product-wp .product .detail {
  width: 460rpx;
}
.product-wp .product .detail .title {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 28rpx;
  color: #041D29;
  line-height: 36rpx;
  margin-bottom: 16rpx;
  height: 72rpx;
}
.product-wp .product .detail .jiage {
  height: 50rpx;
  line-height: 50rpx;
}
.product-wp .product .detail .jiage .price {
  font-size: 36rpx;
  color: #FF5339;
}
.product-wp .product .detail .jiage .unit {
  color: #9B9B9B;
  font-size: 24rpx;
  padding-left: 6rpx;
}
.product-wp .product .detail .tag {
  line-height: 24rpx;
  height: 24rpx;
  margin-bottom: 14rpx;
}
.product-wp .product .detail .tag .tag_r,
.product-wp .product .detail .tag .tag_o,
.product-wp .product .detail .tag .tag_g,
.product-wp .product .detail .tag .tag_b {
  line-height: 24rpx;
  display: inline-block;
  height: 24rpx;
  width: 56rpx;
  border-radius: 6rpx;
  font-size: 9px;
  padding: 0.5rpx 5rpx;
  margin-right: 10rpx;
  text-align: center;
}
.product-wp .product .detail .tag .tag_r {
  color: #FF5339;
  border: 0.5px solid #FF5339;
}
.product-wp .product .detail .tag .tag_o {
  color: #FF8639;
  border: 0.5px solid #FF8639;
}
.product-wp .product .detail .tag .tag_g {
  width: 74rpx;
  color: #9B9B9B;
  border: 0.5px solid #9B9B9B;
}
.product-wp .product .detail .tag .tag_b {
  color: #3795FF;
  border: 0.5px solid #3795FF;
}
.product-wp .product .detail .merchant {
  font-size: 11px;
  color: #9B9B9B;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}
.product-wp .product .detail .merchant .name {
  display: inline-flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.product-wp .product .detail .merchant .name .merchantName {
  width: 163rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.product-wp .product .detail .merchant .name .city {
  margin-left: 20rpx;
}
.product-wp .product .detail .merchant .jindian {
  color: #434343;
}
.product-wp .product .detail .merchant .jindian > text {
  font-size: 11px;
}
page {
  background: #ffffff;
  height: 100%;
  width: 100%;
}
page .search-wp .searchBox {
  height: 88rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 30rpx;
  justify-content: space-between;
}
page .search-wp .searchBox .searchinput {
  position: relative;
  width: 560rpx;
  height: 60rpx;
  padding: 0;
}
page .search-wp .searchBox .searchinput > text {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  z-index: 999;
  /*提高层级 不然不可点击*/
}
page .search-wp .searchBox .searchinput > text.iconsearch {
  font-size: 28rpx;
  left: 20rpx;
  color: #555555;
}
page .search-wp .searchBox .searchinput > text.icondel {
  font-size: 28rpx;
  right: 20rpx;
  color: #cccccc;
}
page .search-wp .searchBox .searchinput > .shurukuang {
  height: 100%;
  width: 100%;
  padding-left: 60rpx;
  background: #EAEAEA;
  border-radius: 76rpx;
  font-size: 28rpx;
  color: #9B9B9B;
}
page .search-wp .searchBox .input {
  color: #9B9B9B;
  font-size: 28rpx;
}
page .search-wp .searchBox .searchbutton {
  width: 120rpx;
}
page .search-wp .searchBox .searchbutton button {
  border-radius: 76rpx;
  width: 100%;
  line-height: 60rpx;
  height: 60rpx;
  background: #3795FF;
  font-size: 28rpx;
  color: #FFFFFF;
}
page .search-wp .hotwords-wp .hotwords {
  padding: 0 30rpx;
}
page .search-wp .hotwords-wp .hotwords .title {
  height: 108rpx;
  line-height: 108rpx;
}
page .search-wp .hotwords-wp .hotwords .title text {
  font-size: 28rpx;
  color: #434343;
}
page .search-wp .hotwords-wp .hotwords .content {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
}
page .search-wp .hotwords-wp .hotwords .content .word {
  margin-bottom: 20rpx;
  margin-right: 20rpx;
  background: #F8F8F8;
  border-radius: 100px;
  font-size: 26rpx;
  color: #434343;
  padding: 12rpx 30rpx;
  text-align: center;
}
page .search-wp .history-wp {
  background: #F8F8F8;
  padding-top: 10rpx;
}
page .search-wp .history-wp .history-record {
  background: #fff;
  padding: 0 30rpx;
}
page .search-wp .history-wp .history-record .title {
  height: 90rpx;
  line-height: 90rpx;
  font-size: 24rpx;
  color: #666;
  border-bottom: 1.1px solid #eee;
}
page .search-wp .history-wp .history-record .history-item {
  width: 100%;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 24rpx;
  color: #999;
  border-bottom: 1.1px solid #eee;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
page .search-wp .history-wp .history-record .history-item > text:first-child {
  width: 624rpx;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
page .search-wp .history-wp .history-record .history-item .iconfont {
  font-size: 20rpx;
}
page .search-wp .history-wp .history-record .clear-all {
  width: 100%;
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
  color: #999;
  font-size: 24rpx;
}
page .search-wp .jieguo-wp .orderbtn {
  height: 88rpx;
  padding: 0 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 28rpx;
  margin-bottom: 20rpx;
}
page .search-wp .jieguo-wp .orderbtn .time {
  color: #9B9B9B;
}
page .search-wp .jieguo-wp .orderbtn .time.active {
  color: #3795FF;
}
page .search-wp .jieguo-wp .orderbtn .line {
  color: #9B9B9B;
  width: 2rpx;
  height: 24rpx;
  background: #EAEAEA;
  margin: 0 36rpx;
}
page .search-wp .jieguo-wp .orderbtn .jiage .price {
  color: #9B9B9B;
}
page .search-wp .jieguo-wp .orderbtn .jiage.active .price {
  color: #3795FF;
}
page .search-wp .jieguo-wp .orderbtn .jiage .icon {
  margin-left: 14rpx;
  width: 20rpx;
  height: 24rpx;
}
page .search-wp .jieguo-wp .jieguolist {
  background: #FFFFFF;
  max-height: 1048rpx;
  overflow: scroll;
}
